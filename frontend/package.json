{
  "name": "saler-frontend",
  "version": "1.0.0",
  "description": "Saler SaaS Platform Frontend - Next.js with TypeScript and Tailwind CSS",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --force",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:fast": "next build --turbo --experimental-turbo",
    "start": "next start -p 3000",
    "start:production": "next start -p 3000 --help",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --ext .js,.jsx,.ts,.tsx src --max-warnings=0",
    "type-check": "tsc --noEmit --strict",
    "type-check:watch": "tsc --noEmit --strict --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:dev": "cross-env ANALYZE=true next build && open dist/bundle-report.html",
    "postbuild": "next-sitemap",
    "export": "next export",
    "prebuild": "npm run type-check",
    "predev": "npm run type-check",
    "optimize": "npm run lint && npm run type-check && npm run test:ci && npm run build:analyze",
    "performance": "npm run build:analyze && npx lighthouse http://localhost:3000 --output-path=./lighthouse-report.html",
    "optimize:performance": "bash scripts/optimize-performance.sh",
    "analyze:bundle": "ts-node scripts/bundle-analyzer.ts",
    "clean": "rm -rf .next dist build out node_modules/.cache tsconfig.tsbuildinfo",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "zod:validate": "node -e \"console.log('Validating Zod schemas...'); const {validateAllSchemas} = require('./src/schemas'); console.table(validateAllSchemas())\"",
    "zod:types": "node -e \"const {generateTypeScriptTypes} = require('./src/schemas'); console.log(generateTypeScriptTypes())\" > src/types/generated-schemas.ts",
    "zod:dev-tools": "next dev --turbo --force",
    "zod:test-schemas": "npm run zod:validate && npm run zod:types && echo 'âœ… Zod schemas validation completed'",
    "zod:check-all": "npm run zod:validate && npm run type-check && echo 'âœ… All Zod and TypeScript checks passed'",
    "dev:enhanced": "npm run zod:dev-tools && echo 'ðŸ”§ Development server with Zod tools ready at http://localhost:3000'"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.3",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "tailwindcss": "3.3.6",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.32",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "clsx": "2.0.0",
    "class-variance-authority": "0.7.0",
    "tailwind-merge": "2.2.0",
    "lucide-react": "0.302.0",
    "react-icons": "4.12.0",
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-navigation-menu": "1.1.4",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-toggle": "1.0.3",
    "@radix-ui/react-tooltip": "1.0.7",
    
    "@tanstack/react-query": "5.17.0",
    "@tanstack/react-query-devtools": "5.17.0",
    "zustand": "4.4.7",
    "react-hook-form": "7.48.2",
    "@hookform/resolvers": "3.3.2",
    "zod": "3.22.4",
    
    "recharts": "2.8.0",
    "chart.js": "4.4.1",
    "react-chartjs-2": "5.2.0",
    
    "react-hot-toast": "2.4.1",
    "sonner": "1.3.1",
    
    "date-fns": "3.0.6",
    "lodash-es": "4.17.21",
    "axios": "1.6.2",
    "ky": "1.2.0",
    "framer-motion": "10.16.16",
    "react-intersection-observer": "9.5.3",
    
    "next-auth": "4.24.5",
    "@next-auth/prisma-adapter": "1.0.7",
    
    "react-dropzone": "14.2.3",
    "react-image-crop": "11.0.1",
    
    "next-intl": "3.4.0",
    
    "next-sitemap": "4.2.3",
    "@vercel/analytics": "1.1.1",
    
    "cross-env": "7.0.3",
    "husky": "8.0.3",
    "lint-staged": "15.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.12",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-next": "1.0.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.7",
    
    "jest": "29.7.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/jest-dom": "6.1.6",
    "@testing-library/user-event": "14.5.1",
    "jest-environment-jsdom": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    
    "@next/bundle-analyzer": "14.0.4",
    "cross-env": "7.0.3",
    "webpack-bundle-analyzer": "4.10.1",
    "compression-webpack-plugin": "11.0.0",
    "source-map-explorer": "2.5.3",
    "lighthouse": "11.4.0",
    "terser-webpack-plugin": "5.3.9",
    "css-minimizer-webpack-plugin": "6.0.0",
    "pre-commit": "0.0.2",
    "ts-node": "10.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}