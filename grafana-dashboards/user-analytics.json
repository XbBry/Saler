          "rowsFrame": {
            "mode": "index",
            "index": 0
          },
          "showValue": "never",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "xAxis": {
            "mode": "series",
            "show": true
          },
          "yAxis": {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        }
      },
