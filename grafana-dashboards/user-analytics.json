{
  "dashboard": {
    "id": null,
    "title": "تحليلات المستخدمين - User Analytics",
    "tags": ["saler", "monitoring", "analytics", "users", "business"],
    "timezone": "Asia/Riyadh",
    "refresh": "1m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "المستخدمين النشطين - Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(active_users_total)",
            "legendFormat": "Current Active Users",
            "refId": "A"
          },
          {
            "expr": "sum(increase(user_sessions_total[24h]))",
            "legendFormat": "Sessions Today",
            "refId": "B"
          },
          {
            "expr": "sum(increase(user_sessions_total[1h]))",
            "legendFormat": "Sessions Last Hour",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "color-background"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 1000}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "text": {},
          "textMode": "value_and_name"
        }
      },
      {
        "id": 2,
        "title": "معدل التحويل - Conversion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(conversions_total[24h])) / sum(increase(user_events_total[24h])) * 100",
            "legendFormat": "Conversion Rate",
            "refId": "A"
          },
          {
            "expr": "sum(increase(signups_total[24h])) / sum(increase(visits_total[24h])) * 100",
            "legendFormat": "Signup Rate",
            "refId": "B"
          },
          {
            "expr": "sum(increase(purchases_total[24h])) / sum(increase(cart_views_total[24h])) * 100",
            "legendFormat": "Purchase Rate",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "color-background"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "green", "value": 5}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "text": {},
          "textMode": "value_and_name"
        }
      },
      {
        "id": 3,
        "title": "القيمة المتوسطة للطلب - Average Order Value",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(order_value_avg)",
            "legendFormat": "Average Order Value",
            "refId": "A"
          },
          {
            "expr": "sum(order_revenue_total[24h]) / sum(increase(orders_completed_total[24h]))",
            "legendFormat": "Daily Avg Order Value",
            "refId": "B"
          },
          {
            "expr": "sum(increase(order_revenue_total[7d])) / sum(increase(orders_completed_total[7d]))",
            "legendFormat": "Weekly Avg Order Value",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "color-background"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            },
            "unit": "currencySAR"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "text": {},
          "textMode": "value_and_name"
        }
      },
